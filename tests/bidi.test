-- Weak processing.

-- W1.  Force single right-to-left isolating run sequence.

BIDIWEAKR
<al><nsm><nsm>
( R R R )

BIDIWEAKR
<nsm>
( R )

BIDIWEAKR
<lri><nsm>
( LRI ON )

BIDIWEAKR
<pdi><nsm>
( PDI ON )

-- W2.  Force single right-to-left isolating run sequence.

BIDIWEAKR
<al><en>
( R AN )

BIDIWEAKR
<al>" "<en>
( R WS AN )

BIDIWEAKR
<pdi><en>
( PDI EN )

BIDIWEAKR
<al><l><rli><en>
( R L RLI L )

BIDIWEAKR
<al><r><fsi><en>
( R R FSI EN )

-- W4.  Use initial R to establish right-to-left context.

BIDIWEAK
<r><en><es><en>
( R EN EN EN )

BIDIWEAK
<r><en><es><es><en>
( R EN ON ON EN )

BIDIWEAK
<r><en><cs><en>
( R EN EN EN )

BIDIWEAK
<r><en><cs><cs><en>
( R EN ON ON EN )

BIDIWEAK
<r><an><cs><an>
( R AN AN AN )

BIDIWEAK
<r><an><cs><cs><an>
( R AN ON ON AN )


-- W5.  Use initial R to establish right-to-left context.

BIDIWEAK
<r><et><et><en>
( R EN EN EN )

BIDIWEAK
<r><en><et><et>
( R EN EN EN )

BIDIWEAK
<r><an><et><en>
( R AN EN EN )

BIDIWEAK
<r><l><et><et><en>
( R L L L L )

-- W6.  Use initial R to establish right-to-left context.

BIDIWEAK
<r><an><et>
( R AN ON )

BIDIWEAK
<r><l><es><an>
( R L ON AN )

BIDIWEAK
<r><en><cs><an>
( R EN ON AN )

BIDIWEAK
<r><et><an>
( R ON AN )

-- W7.  Force single right-to-left isolating run sequence.

BIDIWEAKR
<l><pdi><en>
( L PDI L )

BIDIWEAKR
<l><es><en>
( L ON L )

BIDIWEAKR
<r><rli><en>
( R RLI EN )


-- Neutral processing.

-- N1/N2

BIDINEUTRAL
<r><l><on><on><l>
( R L L L L )

BIDINEUTRAL
<r><on><r>
( R R R )

BIDINEUTRAL
<r><pdi><an>
( R R AN )

BIDINEUTRAL
<r><on><on><on><en>
( R R R R EN )

BIDINEUTRAL
<an><on><r>
( AN R R )

BIDINEUTRAL
<an><on><an>
( AN R AN )

BIDIWEAK
<l><r><an><on><en>
( L R AN ON EN )

BIDINEUTRAL
<l><r><an><on><en> -- L R context avoids conversion of final EN to L by weak.
( L R AN R EN )

BIDINEUTRAL
<en><lri><pdi><fsi><pdi><rli><pdi><r>
( EN R R R R R R R )

BIDIWEAK
<l><r><en>" "<an>
( L R EN WS AN )

BIDINEUTRAL
<l><r><en>" "<an> -- L R context avoids conversion of EN to L by weak.
( L R EN R AN )

BIDIWEAK
<l><r><en><on><en>
( L R EN ON EN )

BIDINEUTRAL
<l><r><en><on><en>
( L R EN R EN )

